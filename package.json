{
  "name": "raxx-google-maps",
  "version": "0.0.1",
  "description": "Google Map Web Component",
  "main": "dist/raxxgooglemaps.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/raxxgooglemaps.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@stencil/core": "0.7.7",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/googlemaps": "^3.30.8",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "tslib": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rakshans1/raxx-google-maps.git"
  },
  "author": "Rakshan Shetty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rakshans1/raxx-google-maps"
  },
  "homepage": "https://github.com/rakshans1/raxx-google-maps",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
